print("PYTHON\nIZOHLI\nLUGATI\n")
lugat={
       'int':',butun sonlar',
       'float':'onlik sonlar',
       'if':'shart tekshirish',
       'else':'shart bajarilmagan',
       'elif':'else va if qoshimchasi',
       'True':'rost',
       'False':'Yolgon',
       'append':'qoshimcha',
       'title':'birinchi harf kotta',
       'lower':'hammasi kichkina'
       }
# print(lugat.get(input('Python kodini kiriting>>> '),\
#                 'bunaqasini kiritib ulgurmadila'))

soz=input('kalit so"z kiriting>>> ')
tarjima=lugat.get(soz)
if tarjima==None:
    print('bunisi hali kirmagan')
else:
    print(f'{soz.title()} sozi {tarjima} digani' )
